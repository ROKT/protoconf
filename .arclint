{
  "linters": {
    "python": {
      "type": "black",
      "include": ["(\\.py)"],
      "exclude": [
        "(\\_pb2.py)",
        "(\\_grpc.py)"
      ]
    },
    "golang": {
      "type": "govet",
      "include": ["(\\.go)"]
    },
    "buildifier": {
      "type": "buildifier",
      "include": [
        "(^BUILD$)",
        "(\\.bzl$)",
        "(\\.BUILD$)",
        "(^WORKSPACE$)"
      ]
    }
  }
}
